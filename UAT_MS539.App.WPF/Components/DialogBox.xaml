<UserControl x:Class="UAT_MS539.Components.DialogBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:UAT_MS539.Components"
             mc:Ignorable="d" 
             d:DesignHeight="150" d:DesignWidth="600">
    <Grid Background="#7F323232">
        <Grid.Effect>
            <DropShadowEffect/>
        </Grid.Effect>
        <TextBlock x:Name="_label" FontFamily="/UAT_MS539.App.WPF;component/Assets/UI/Fonts/#Quicksand Book" FontSize="22" Foreground="White" Background="#00000000" TextDecorations="{x:Null}" Margin="5" TextWrapping="Wrap">
            <TextBlock.Effect>
                <DropShadowEffect/>
            </TextBlock.Effect>
            [Dialog Line]
        </TextBlock>
    </Grid>
</UserControl>
